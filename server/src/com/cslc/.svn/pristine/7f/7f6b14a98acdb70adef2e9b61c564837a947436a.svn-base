package com.cslc.dao.score;

import java.util.Date;
import com.platform.base.BaseEntity;

public class Score extends BaseEntity {

	public static final byte INOUTTYPE_IN= 0;// 进
	public static final byte INOUTTYPE_OUT = 1;// 出
	
	public static final byte STATUS_ENABLE = 0;// 有效
	public static final byte STATUS_DISABLE = 1;// 无效
	
	private Long accountid;
	private Integer score;
	private Long rewardid;
	private Date createtime;
	private Byte inouttype;
	private Integer category;
	private Long id;
	private String title;
	private Byte status;

	public Integer getCategory() {
		return category;
	}

	public void setCategory(Integer category) {
		this.category = category;
	}

	public Long getAccountid(){
		return accountid;
	}

	public void setAccountid(Long accountid) {
		this.accountid = accountid;
	}

	public Integer getScore(){
		return score;
	}

	public void setScore(Integer score) {
		this.score = score;
	}

	public Long getRewardid(){
		return rewardid;
	}

	public void setRewardid(Long rewardid) {
		this.rewardid = rewardid;
	}

	public Date getCreatetime(){
		return createtime;
	}

	public void setCreatetime(Date createtime) {
		this.createtime = createtime;
	}

	public Byte getInouttype(){
		return inouttype;
	}

	public void setInouttype(Byte inouttype) {
		this.inouttype = inouttype;
	}

	public Long getId(){
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public String getTitle(){
		return title;
	}

	public void setTitle(String title) {
		this.title = title;
	}

	public Byte getStatus(){
		return status;
	}

	public void setStatus(Byte status) {
		this.status = status;
	}

}