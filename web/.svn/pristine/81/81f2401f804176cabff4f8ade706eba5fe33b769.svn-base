/*宸茶喘浜烘暟*/
.top-left {
    position: absolute;
    top: 0px;
    left: 0px;
    font-size: 16px;
    font-weight: 500;
    padding: 7px 14px;
}
.top-right {
    position: absolute;
    top: 0px;
    right: 0px;
    font-size: 12px;
    color: #a4a4a4;
    padding: 5px 20px;
}
.top-right span {
    color: #e93c14;
    font-size: 16px;
    padding: 0 2px;
    letter-spacing: 2px;
}
.bottom-left {
    position: absolute;
    font-size: 15px;
    bottom: 0px;
    left: 0px;
    padding: 10px 10px;
}
.bottom-left img {
    width: 11px;
    height: 11px;
}
.bottom-left span {
    font-size:12px;
    color: #a4a4a4;
    letter-spacing: 1px;
}
.bottom-right {
    position: absolute;
    bottom: 0px;
    right: 0px;
    font-size:12px;
    color: #a4a4a4;
    padding: 10px 13px;
}
.bottom-right span {
    padding: 0 3px 0 3px;
    font-size: 14px;
    letter-spacing: 2px;
    color: #f4b817;
}

